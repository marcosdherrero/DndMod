{
  "schemaVersion": 1,
  "id": "dndmod",
  "version": "${version}",

  "name": "DnD Mod",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Marcos!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "Marcos License",
  "icon": "assets/dndmod/textures/icon.JPG",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.marcos.dndmod.DnDMod"
    ],
    "client": ["net.marcos.dndmod.DnDModClient"]
  },

  "mixins": [
    "dndmod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
